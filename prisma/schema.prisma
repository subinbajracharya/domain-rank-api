generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Ranking {
  id        String   @id @default(cuid())
  domain    String
  date      String   // YYYY-MM-DD
  rank      Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([domain])
  @@index([date])
  @@unique([domain, date])
}
